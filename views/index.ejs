<script type="text/javascript">
	var infowindow = new google.maps.InfoWindow();
	var cityList = [ [ 'San Jose', 37.3382, -121.8863, 3 ],
			[ 'San Francisco', 37.7833, -122.4167, 1 ],
			[ 'Los Angeles', 34.0500, -118.2500, 4 ],
			[ 'Irvine', 33.6694, -117.8231, 8 ],
			[ 'San Diego', 32.7150, -117.1625, 5 ],
			[ 'New York', 40.7127, -74.0059, 2 ],
			[ 'Boston', 42.3601, -71.0589, 6 ],
			[ 'Houston', 29.7604, -95.3698, 7 ],
			[ 'Chicago', 41.850033, -87.6500523, 9 ] ], demoCenter = new google.maps.LatLng(
			41, -87), map;

	function initialize() {
		map = new google.maps.Map(document.getElementById('map_canvas'), {
			zoom : 3,
			center : demoCenter,
			mapTypeId : google.maps.MapTypeId.ROADMAP
		});
	}

	function addMarkers() {
		var marker, i, infowindow = new google.maps.InfoWindow();

		for (i = 0; i < cityList.length; i++) {
			marker = new google.maps.Marker({
				position : new google.maps.LatLng(cityList[i][1],
						cityList[i][2]),
				map : map,
				title : cityList[i][0]
			});

			google.maps.event.addListener(marker, 'click',
					(function(marker, i) {
						return function() {
							//alert(i)
							window.location = "./data/alldata?location_id="
									+ (i + 1);
						}
					})(marker, i));

			google.maps.event.addListener(marker, 'mouseover', (function(
					marker, i) {
				return function() {
					infowindow.setContent(cityList[i][0]);
					infowindow.open(map, this);
				}
			})(marker, i));

			// assuming you also want to hide the infowindow when user mouses-out
			google.maps.event.addListener(marker, 'mouseout', (function(marker,
					i) {
				return function() {
					infowindow.close();
				}
			})(marker, i));
		}
	}

	$(document).ready(function() {
		initialize();
		addMarkers();
	});
</script>

